    {
      "parameters": {
        "jsCode": "// Собираем все обработанные ключевые слова для обновления статуса\nconst items = $input.all();\nconst processedKeywords = new Set();\n\n// Собираем уникальные key_word_id из всех обработанных запросов\nitems.forEach(item => {\n  if (item.json.original_key_word_id) {\n    processedKeywords.add(item.json.original_key_word_id);\n  }\n});\n\n// Возвращаем массив для обновления статуса\nreturn Array.from(processedKeywords).map(keyWordId => ({\n  json: {\n    key_word_id: keyWordId\n  }\n}));"
      },
